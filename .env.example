# Web server
PORT=8080

# Basic auth for WebUI
BASIC_AUTH_USER=admin
BASIC_AUTH_PASS=change_me

# Allow self-signed certs when talking to Unraid servers
UNRAID_ALLOW_SELF_SIGNED=true

# Wake-on-LAN
WOL_BROADCAST=255.255.255.255
WOL_INTERFACE=eth0

# Optional: set your own CSRF secret (otherwise random each boot)
# CSRF_SECRET=change_me_long_random
